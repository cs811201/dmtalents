LINK XFORM "Check_Macros" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 
 GLOBAL_VAR  ErrMsg			!ModelVariable
 GLOBAL_VAR  VarName_inSearch
 GLOBAL_VAR  Path2Model
 GLOBAL_VAR  MacroNames
 GLOBAL_VAR  FoundMacroLocations
 
 
 ErrMsg = ""
 
 !===check all Macros for Usage of Variable==============================================================
 FoundMacroLocations = ""
 
 !---identify Macros
 ICCAP_FIND_CHILDREN VAL$(Path2Model), "Macro", MacroNames
 
 !---look for VarName in found Macros
 FOR iMacro=0 TO SIZEOF(MacroNames)-1
   Path2Macro=VAL$(Path2Model)&"/"&VAL$(MacroNames[iMacro])
   !---import the contents of the Macro
   MacroContents = LOOKUP_TABLE_VAL(VAL$(Path2Macro), "Text", ErrMsg)	   !"Text" is the keyword to get all the Macro PEL code into the Variable 'x'
   IF VAL$(ErrMsg)=="" THEN   !it's PEL and *NOT* Python
     IF STRPOS(VAL$(MacroContents), VAL$(VarName_inSearch)) > 0 THEN
       DIMENSION_ARRAY(FoundMacroLocations, SIZEOF(FoundMacroLocations)+1)
       FoundMacroLocations[SIZEOF(FoundMacroLocations)-1]=VAL$(Path2Macro)
     END IF
   ELSE
     ErrMsg=""
   END IF
 NEXT iMacro
 
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
