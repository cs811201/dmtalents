LINK XFORM "Create_GUI" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 
 GLOBAL_VAR  VarPath
 GLOBAL_VAR  VarName
 
 
 !First specify some useful local variables
 ParentName="./GUI_Results"
 ChildName="TL_VarPath_VarVal"
 Path2Child=VAL$(ParentName)&"."&VAL$(ChildName)
 
 
 !=====Remove existing child GUI======================
 ICCAP_FUNC("./"&VAL$(Path2Child),"Delete")
 
 !=====Create a new child GUI (a table)===============
 ChildObject="TS"	!###>ScrollTable
 ICCAP_FUNC(ParentName,"Add GUI",ChildName,ChildObject)
 
 ParentName=VAL$(ParentName)&"."&VAL$(ChildName)
 
 
 
 
 
 i=0
 WHILE i < SIZEOF(VarPath)
   !---Add Horizontal Table
   ChildName   ="TL_"&VAL$(i)
   ChildObject ="TL"	!###>Table
   ICCAP_FUNC(ParentName, "Add GUI", ChildName, ChildObject)
 
   !---Specify Table Orientation, Frame Visible, Fixwed Height
   Path2Table=VAL$(ParentName)&"."&VAL$(ChildName)
   ICCAP_FUNC(Path2Table, "Set GUI Options", "OR=HO", "FV=T", "TO=FH", "")
 
   !---Create Label VarPathName
   ChildName   ="LA_VarPathName"
   ChildObject ="LA"	!###>Lable
   ICCAP_FUNC(Path2Table, "Add GUI", ChildName, ChildObject)
 
   !---Specify Lable Caption
   Path2Child=VAL$(Path2Table)&"."&VAL$(ChildName)
   ICCAP_FUNC(Path2Child, "Set GUI Options", "CA="&VAL$(VarPath[i])&"/"&VAL$(VarName), "")
 
   !---Create EditField VarVal
   ChildName   ="ED_VarVal"
   ChildObject ="ED"	!###>EditField
   ICCAP_FUNC(Path2Table, "Add GUI", ChildName, ChildObject)
 
   !---Specify Edit Field Value
   Path2Child=VAL$(Path2Table)&"."&VAL$(ChildName)
   ICCAP_FUNC(Path2Child, "Set GUI Options", "VA=%VarVal["&VAL$(i)&"]", "")
   !---Specify Callback
   ICCAP_FUNC(Path2Child, "Set GUI Callback", "EP", "gui_Apply", "Execute,"&VAL$(i))
 
   !---Create Apply-Button
   ChildName   ="PB_Apply"
   ChildObject ="PB"	!###>PushButton
   ICCAP_FUNC(Path2Table, "Add GUI", ChildName, ChildObject)
 
   !---Specify Caption
   Path2Child=VAL$(Path2Table)&"."&VAL$(ChildName)
   ICCAP_FUNC(Path2Child, "Set GUI Options", "CA=Apply Value Change", "TO=FW", "")
 
   !---Specify Callback
   ICCAP_FUNC(Path2Child, "Set GUI Callback", "AC", "gui_Apply", "Execute,"&VAL$(i))
 
 
