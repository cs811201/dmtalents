LINK XFORM "Find_Variables" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 
 GLOBAL_VAR  ErrMsg
 GLOBAL_VAR  Path2Variables
 GLOBAL_VAR  FoundVariables
 GLOBAL_VAR  FoundVariables_sel
 
 
 
 FoundVariables=""
 N=0
 i=0
 ErrMsg=""
 WHILE VAL$(ErrMsg)==""
   VarName  = LOOKUP_TABLE_VAL(VAL$(Path2Variables), "Name["&VAL$(i)&"]", ErrMsg)
   VarValue = LOOKUP_TABLE_VAL(VAL$(Path2Variables), "Value["&VAL$(i)&"]", ErrMsg)
   IF (VAL$(ErrMsg)=="") ANDALSO (VAL$(VarName)<>"") ANDALSO (STRPOS(VAL$(VarName), "[")==0) THEN
     DIMENSION_ARRAY("FoundVariables", N+1)
     !---add marker [] if it's an ICCAP_ARRAY
     IF STRPOS(VAL$(VarValue), "[")>0 THEN VarName = VAL$(VarName)&"[ ]"
     FoundVariables[N] =VarName
     N = N + 1
   END IF
   i = i + 1
 END WHILE
 
 
 IF SIZEOF(FoundVariables)==0 THEN
   DIMENSION_ARRAY(FoundVariables, 1)
   FoundVariables[0] = "=> No Variables !!! <="
 END IF
 
 FoundVariables_sel = -1
 
 RETURN_VALUE 1
}
