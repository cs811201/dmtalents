LINK XFORM "gui_Check_Macro_Xfm_Gui" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 
 !---this Program checks for 'VarName_inSearch' in Macros, Programs and GUIs
 GLOBAL_VAR  FoundVariables
 GLOBAL_VAR  FoundVariables_sel
 GLOBAL_VAR  VarName_inSearch
 GLOBAL_VAR  VarTypes_sel
 GLOBAL_VAR  Manage_GUI_FoundMacrosList
 GLOBAL_VAR  Manage_GUI_FoundXfmList
 GLOBAL_VAR  Manage_GUI_FoundGuiList
 GLOBAL_VAR  Manage_GUI_DeleteVariable
 GLOBAL_VAR  Path2Model
 GLOBAL_VAR  DutNames
 GLOBAL_VAR  DutNames_sel
 GLOBAL_VAR  SetupNames
 GLOBAL_VAR  SetupNames_sel
 GLOBAL_VAR  FoundXfmLocations
 GLOBAL_VAR  FoundXfmLocations_sel
 GLOBAL_VAR  FoundMacroLocations
 GLOBAL_VAR  FoundMacroLocations_sel
 GLOBAL_VAR  FoundGuiLocations
 GLOBAL_VAR  FoundGuiLocations_sel
 
 
 
 FoundXfmLocations_sel   = -1
 FoundMacroLocations_sel = -1
 FoundGuiLocations_sel   = -1
 FoundXfmLocations   = ""
 FoundMacroLocations = ""
 FoundGuiLocations   = ""
 Manage_GUI_DeleteVariable = 0
 
 IF VAL(FoundVariables_sel)<0 THEN RETURN_VALUE 0
 
 
 VarName_inSearch = VAL$(FoundVariables[FoundVariables_sel])
 !---get rid of ICCAP_ARRAY indicator []
 VarName_inSearch = TRIM$(VAL$(VarName_inSearch), "[ ]")
 
 !---ModelVariable
 IF VarTypes_sel == 0 THEN
   !---search in Macros
   ICCAP_FUNC("Check_Macros", "Execute")
   !---search in Model Guis
   ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Model))
 
   !---search in Dut/Setup Guis and in all Transforms of the Model
   ICCAP_FIND_CHILDREN VAL$(Path2Model), "DUT", DutNames
   FOR iDut=0 TO SIZEOF(DutNames)-1
     Path2Dut=VAL$(Path2Model)&"/"&VAL$(DutNames[iDut])
     !---search in Dut Guis
     ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Dut))
 
     ICCAP_FIND_CHILDREN VAL$(Path2Dut), "Setup", SetupNames
     FOR iSetup=0 TO SIZEOF(SetupNames)-1
       Path2Setup=VAL$(Path2Dut)&"/"&VAL$(SetupNames[iSetup])
       !---search in Transforms
       ICCAP_FUNC("Check_Transforms", "Execute", VAL$(Path2Setup))
       !---search in Setup Guis
       ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Setup))
     NEXT iSetup
   NEXT iDut
   Manage_GUI_FoundMacrosList = 1
   Manage_GUI_FoundXfmList = 1
 END IF
 
 
 !---DutVariable: search in Dut/Setup Guis and in all Transforms of all Setups of the Dut
 IF VarTypes_sel == 1 THEN
   Path2Dut=VAL$(Path2Model)&"/"&VAL$(DutNames[DutNames_sel])
   !---search in Dut Guis
   ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Dut))
   Manage_GUI_FoundGuiList = 1
 
   ICCAP_FIND_CHILDREN VAL$(Path2Dut), "Setup", SetupNames
   FOR iSetup=0 TO SIZEOF(SetupNames)-1
     Path2Setup=VAL$(Path2Dut)&"/"&VAL$(SetupNames[iSetup])
     !---search in Transforms
     ICCAP_FUNC("Check_Transforms", "Execute", VAL$(Path2Setup))
     !---search in Setup Guis
     ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Setup))
     Manage_GUI_FoundGuiList = 1
   NEXT iSetup
   Manage_GUI_FoundMacrosList = 0
   Manage_GUI_FoundXfmList = 1
 END IF
 
 
 !---SetupVariable: search Transforms of the Setup
 IF VarTypes_sel == 2 THEN
   Path2Setup=VAL$(Path2Model)&"/"&VAL$(DutNames[DutNames_sel])&"/"&VAL$(SetupNames[SetupNames_sel])
   !---search in Transforms
   ICCAP_FUNC("Check_Transforms", "Execute", VAL$(Path2Setup))
   !---search in Setup Guis
   ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Setup))
   Manage_GUI_FoundMacrosList = 0
   Manage_GUI_FoundXfmList = 1
   Manage_GUI_FoundGuiList = 1
 END IF
 
 
 !---if no Macros/Xfms/Guis with selected Variable have been found, 
 !     enable the user to delete this Variable
 IF (SIZEOF(FoundMacroLocations)==0) ANDALSO (SIZEOF(FoundXfmLocations)==0) ANDALSO(SIZEOF(FoundGuiLocations)==0) THEN
   IF VAL$(FoundVariables[0]) <> "=> No Variables !!! <=" THEN
     Manage_GUI_DeleteVariable=1
   END IF
 END IF
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
LINK XFORM "gui_Delete_Variable" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 GLOBAL_VAR  VarName_inSearch
 GLOBAL_VAR  Path2Variables
 GLOBAL_VAR  Manage_GUI_DeleteVariable
 GLOBAL_VAR  ErrMsg
 GLOBAL_VAR  VarTypes_sel
 GLOBAL_VAR  FoundMacroLocations
 GLOBAL_VAR  FoundXfmLocations
 GLOBAL_VAR  FoundGuiLocations
 
 
 !---check, if this Variable really exists !!!
 ErrMsg=""
 dummy=LOOKUP_VAR(VAL$(Path2Variables)&"/"&VAL$(VarName_inSearch), ErrMsg)
 
 IF VAL$(ErrMsg)=="" THEN
   GET_STRING "Next: checking if the variable is used in other locations, applying paths  [OK]", dummy
   VarTypes_sel_orig = VAL(VarTypes_sel)
   VarTypes_sel = 0
   ICCAP_FUNC("gui_Check_Macro_Xfm_Gui", "Execute")
   VarTypes_sel = VarTypes_sel_orig
 
   IF (SIZEOF(FoundMacroLocations)==0) ANDALSO (SIZEOF(FoundXfmLocations)==0) ANDALSO (SIZEOF(FoundGuiLocations)==0) THEN
 
     GET_STRING "WARNING: Variables named by CAPITAL characters may be SystemVariables !!!
       Enter Delete, or Keep", "Keep", Answer
     IF LWC$(VAL$(Answer))=="delete" THEN
       !---delete the variable in the Model
       ICCAP_FUNC(VAL$(Path2Variables), "SetTableFieldValue", "Name["&VAL$(VarName_inSearch)&"]", "") 
       !---update the GUI
       ICCAP_FUNC("Find_Variables", "Execute")
     END IF
   ELSE
     GET_STRING "Variable is applied in other locations, using rel. paths !!
      ==>  See the currently displayed locations !!
      ==>  It cannot be deleted !!!   [OK]", dummy
   END IF
 ELSE
   GET_STRING "ERROR: the requested Variable '"&VAL$(VarName_inSearch)&"' does not exist and therefore, cannot be deleted  [OK]", dummy
 END IF
 
 Manage_GUI_DeleteVariable = 0
 ICCAP_FUNC("gui_Find_Variables_Mgr", "Execute")
 ICCAP_FUNC("gui_Check_Macro_Xfm_Gui", "Execute")
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
