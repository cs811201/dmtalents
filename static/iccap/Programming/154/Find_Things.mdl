LINK XFORM "gui_Search" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 
 GLOBAL_VAR  ErrMsg			!ModelVariable
 GLOBAL_VAR  Path2Model
 GLOBAL_VAR  SearchString
 GLOBAL_VAR  MacroNames
 GLOBAL_VAR  FoundMacroLocations
 GLOBAL_VAR  FoundMacroLocations_sel
 GLOBAL_VAR  DutNames
 GLOBAL_VAR  SetupNames
 GLOBAL_VAR  XfmNames
 GLOBAL_VAR  FoundXfmLocations
 GLOBAL_VAR  FoundXfmLocations_sel
 GLOBAL_VAR  FoundGuiLocations
 GLOBAL_VAR  FoundGuiLocations_sel
 GLOBAL_VAR  txt_Notes
 
 
 ErrMsg=""
 txt_Notes = "Searching ..."
 
 
 !~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 !---reset Variables
 FoundMacroLocations = ""	
 FoundXfmLocations   = ""	
 FoundGuiLocations   = ""	
 FoundMacroLocations_sel = -1
 FoundXfmLocations_sel   = -1
 FoundGuiLocations_sel   = -1
 
 
 !===check all Macros==============================================================
 !---identify Macros
 ICCAP_FIND_CHILDREN VAL$(Path2Model), "Macro", MacroNames
 
 !---look for SearchString in found Macros
 FOR iMacro=0 TO SIZEOF(MacroNames)-1
   Path2Macro="Macro "&VAL$(Path2Model)&"/"&VAL$(MacroNames[iMacro])
   !---import the contents of the Macro
   MacroContents = LOOKUP_TABLE_VAL(VAL$(Path2Macro), "Text")	   !"Text" is the keyword to get all the Macro PEL code into the Variable 'x'
     
   IF STRPOS(VAL$(MacroContents), VAL$(SearchString)) > 0 THEN
     DIMENSION_ARRAY(FoundMacroLocations, SIZEOF(FoundMacroLocations)+1)
     FoundMacroLocations[SIZEOF(FoundMacroLocations)-1]=VAL$(Path2Macro)
   END IF
 NEXT iMacro
 
 
 
 !===check all Transforms==============================================================
 !---identify Transforms with functionality 'Program' or 'Program2'
 ICCAP_FIND_CHILDREN VAL$(Path2Model), "DUT", DutNames
 FOR iDut=0 TO SIZEOF(DutNames)-1
   Path2Dut=VAL$(Path2Model)&"/"&VAL$(DutNames[iDut])
 
   ICCAP_FIND_CHILDREN VAL$(Path2Dut), "Setup", SetupNames
   FOR iSetup=0 TO SIZEOF(SetupNames)-1
     Path2Setup=VAL$(Path2Dut)&"/"&VAL$(SetupNames[iSetup])
 
     ICCAP_FIND_CHILDREN VAL$(Path2Setup), "Transform", XfmNames
     FOR iXfm=0 TO SIZEOF(XfmNames)-1
       Path2Xfm="Transform "&VAL$(Path2Setup)&"/"&VAL$(XfmNames[iXfm])
 
       !---verify if it's a Program or Program2
       x = LOOKUP_TABLE_VAL(VAL$(Path2Xfm), "Function")
       IF (VAL$(x)=="Program") OR (VAL$(x)=="Program2") THEN
      
         !---import the contents of the Program			! always apply the keyword 'Program' to access the PEL *editor* code, 
         ProgramContents=LOOKUP_TABLE_VAL(VAL$(Path2Xfm), "Program")	!   independently of wether you are using the *function* 'Program' or 'Program2'.
 
         !---find out if search string is part of currently loaded program
         IF STRPOS(VAL$(ProgramContents), VAL$(SearchString)) > 0 THEN
           DIMENSION_ARRAY(FoundXfmLocations, SIZEOF(FoundXfmLocations)+1)
           FoundXfmLocations[SIZEOF(FoundXfmLocations)-1] = VAL$(Path2Xfm)
         END IF
       END IF
     NEXT iXfm
   NEXT iSetup
 NEXT iDut
 
 
 !===check all GUIs==============================================================
 !---Model Guis
 ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Model))
 !---Dut & Setup Guis
 ICCAP_FIND_CHILDREN VAL$(Path2Model), "DUT", DutNames
 FOR iDut=0 TO SIZEOF(DutNames)-1
   Path2Dut=VAL$(Path2Model)&"/"&VAL$(DutNames[iDut])
   ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Dut))
   ICCAP_FIND_CHILDREN VAL$(Path2Dut), "Setup", SetupNames
   FOR iSetup=0 TO SIZEOF(SetupNames)-1
     Path2Setup=VAL$(Path2Dut)&"/"&VAL$(SetupNames[iSetup])
     ICCAP_FUNC("Check_Gui", "Execute", VAL$(Path2Setup))
   NEXT iSetup
 NEXT iDut
 
 
 !---reset note
 txt_Notes = ""
 
 
 !---display result
 IF (SIZEOF(FoundMacroLocations)>0) ORELSE (SIZEOF(FoundXfmLocations)>0) ORELSE (SIZEOF(FoundGuiLocations)>0) THEN 
   ICCAP_FUNC("./GUI_Results", "Display Modeless GUI", "Found Locations of Text")
 ELSE
   GET_STRING "Requested String '"&VAL$(SearchString)&"' was not found   [OK]", dummy
 END IF
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
LINK XFORM "Check_Gui" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 !---this Program populates array FoundGuiLocations
 
 
 
 GLOBAL_VAR  SearchString
 GLOBAL_VAR  FoundGuiLocations
 GLOBAL_VAR  tmpLine
 
 
 GET_STRING "Path2Gui", Path2Gui
 
 !---save all GUIs of current path, altogether to a file
 ICCAP_FUNC(VAL$(Path2Gui)&"/GUIItems", "Save As", "tmp.gui")
 
 !---open file
 FileID = USERC_open("tmp.gui", "r")
 
 !---start loop off
 RetVal=1 
 WHILE RetVal>=0
   tmpLine=""
   RetVal = USERC_readstr(FileID, 0, "%[^\r\n]", tmpLine)
 
   !---verify if VarName in search is part of currently read line
   IF STRPOS(VAL$(tmpLine), VAL$(SearchString))>0 THEN 
     RetVal=ARRAY_INSERT_AT(FoundGuiLocations, Path2Gui)
     BREAK
   END IF
 
   RetVal=1 ! even if last RetVal was negative, there may just be a blank line
   WHILE RetVal>=0 AND VAL$(tmpLine)<>CHR$(10)
      RetVal=USERC_readstr(FileID, 0, "%1[\n\r]", tmpLine)
   END WHILE
      
 END WHILE
 
 !---close file
 dummy = USERC_close(FileID)
 
 
 
 
 RETURN_VALUE 1
 
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
