element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 GLOBAL_VAR  Debug
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI("PartName")&"..."
 
 GLOBAL_VAR  Path2Model      !ModelVariable
 GLOBAL_VAR  DutNames
 GLOBAL_VAR  SetupNames
 GLOBAL_VAR  VarName
 GLOBAL_VAR  VarFound
 GLOBAL_VAR  VarPath
 GLOBAL_VAR  VarVal
 GLOBAL_VAR  iFound
 
 
 !---reset variables
 VarPath=""
 VarVal=""
 iFound=0
 VarFound = 0
 
 IF VAL$(VarName)=="" THEN 
   GET_STRING "ERROR: no Variable Name was entered !  [OK]", dummy
   RETURN_VALUE -1
 END IF
 
 ICCAP_FIND_CHILDREN VAL$(Path2Model), "DUT", DutNames
 ICCAP_FUNC("EchoPathVariable", "Execute", VAL$(Path2Model), VarName)
 
 k=0
 WHILE k < DutNames
   dutpath = VAL$(Path2Model)&"/"&VAL$(DutNames[k])
   ICCAP_FIND_CHILDREN dutpath, "Setup", SetupNames
     
   ICCAP_FUNC("EchoPathVariable", "Execute", dutpath, VarName)
     
   l=0
   WHILE l<SetupNames
     setpath = dutpath&"/"&VAL$(SetupNames[l])
     ICCAP_FUNC("EchoPathVariable", "Execute", setpath, VarName)
   l=l+1
   END WHILE
     
   k=k+1
 END WHILE
   
 IF VarFound==0 THEN 
   GET_STRING "Variable "&VAL$(VarName)&" not found", dummy
 ELSE
   ICCAP_FUNC("Create_GUI", "Execute")
   ICCAP_FUNC("./GUI_Results", "Display Modeless GUI", "Found Variable Locations and their Values")
 END IF
 
 VarFound = 0
 
 RETURN_VALUE 1
 
}
