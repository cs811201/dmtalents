LINK XFORM "EXAMPLES" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 !============================================================================
 !NOTE:
 !THE PATH TO THE MAIN GUI ITEM IS PER "/", 
 !THE PATH DOWN THE GUI ITEMS IS PER "."
 !============================================================================
 
 
 !OPEN AND CLOSE GUIS FROM A PEL PROGRAM
 !===============================================================
 
 !DISPLAY A GUI
 !===========
 ! IMPORTANT NOTE:
 ! when different GUIs are opened at the same time, 
 ! make sure they have individual WindowTitles.
 ! If not, the updating of the contents of the GUIs may not work 
 ! (e.g. updating of Parameter/Variable values etc.)
 
 ! NOTE: there are modal and modeless GUIs available
 !	- modeless GUI: the PEL program will continue with the GUI open
 !	- modal GUI   : the PEL program will halt until the opened GUI is destroyed/closed
 
 !display a GUI with a title in the window frame
 !-------------------------------------------
 ICCAP_FUNC("./myGUI", "Display Modeless GUI", "WindowTitle")
 ICCAP_FUNC("./myGUI", "Display Modal GUI", "WindowTitle")
 
 
 
 !display without a title in the window frame
 !-------------------------------------------
 ICCAP_FUNC("./myGUI", "Display Single Modeless GUI")
 ICCAP_FUNC("./myGUI", "Display Single Modal GUI")
 
 
 !---------------------------------------------------------
 
 
 
 !DESTROY A GUI
 !==============
 !GUIs with a title in the window frame
 ICCAP_FUNC("./myGUI", "Destroy GUI", "WindowTitle")
 
 
 
 !GUIs without a title in the window frame
 ICCAP_FUNC("./myGUI", "Destroy Single GUI")
 
 
 
 !COPY GUIs
 !=============
 ! you can also copy (parts) of a GUI:
 ! to copy into a new GUI top-level-item /mymodel/myGUI.newgui
 ! ICCAP_FUNC("/mymodel/myGUI.subgui", "Copy", "/mymodel/newgui") 
 
 ! to copy into the hierarchy of an existing GUI 
  ICCAP_FUNC("/mymodel/myGUI.subgui", "Copy", "/mymodel/myGUI.subgui.newGUI") 
 
 
 
 !You can also destroy a GUI from within a GUI
 !============================================
 !In the GUI, define a Push Button, with the callback 
 !Button Pressed:                      ./NAME_OF_THE_GUI
 !and the Button-Pressed Function:     Destroy Single GUI
 !
 !if the GUI had no "WindowTitle" name,
 !Button Pressed:                      ./NAME_OF_THE_GUI
 !and the Button-Pressed Function:     Destroy GUI,WindowTitle      (no blanks !!!)
 !
 !NOTE:	this destroy-button will only work, 
 !	if the GUI has been opened by an ICCAP_FUNC 
 !	in a PEL. It does not work if the GUI
 !	was opened manually from the GUI tab (!)
 
 
 
 
 
 !save all GUIs on the Model level
 !NOTE: GUIs can only be saved completely, per Model, DUT or Setup GUI
 !--------------------------------
 ICCAP_FUNC("/Model/GUIItems", "Save As")
 
 !load a .gui file on the Model level
 !-----------------------------------
 ICCAP_FUNC("/Model/GUIItems", "Open")
 
 
 
 
 
 !save all Setup GUIs
 !NOTE: GUIs can only be saved completely, per Model, DUT or Setup GUI
 !-------------------
 ICCAP_FUNC("/Model/DUT/Setup/GUIItems", "Save As")
 
 
 !load a .gui file on the Setup level
 !-----------------------------------
 ICCAP_FUNC("/Model/DUT/Setup/GUIItems", "Open")
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
