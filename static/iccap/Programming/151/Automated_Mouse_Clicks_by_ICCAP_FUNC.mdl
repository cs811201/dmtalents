data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 !LOAD A MODEL FILE INTO ICCAP/Main
 !---------------------------------
 ICCAP_FUNC("/", "Read Model", "MyModelName.mdl")
 
 
 !CHANGE WORKING DIRECTORY
 !------------------------
 working_directory=SYSTEM$("pwd")			  !returns current location of directory pointer
 ICCAP_FUNC("/", "Change Directory", "/tmp") 		  !change to directory /tmp
 ICCAP_FUNC("/", "Change Directory", VAL$(working_directory))  !change back to previous working directory
 
 
 !EXIT ICCAP
 !----------
 ICCAP_FUNC("/", "Exit")   !you will be asked if you really want to exit and to save open files
 ICCAP_FUNC("/", "Exit!")  ! THIS IS THE KILLER COMMAND !!!!!!!!!!!
 
 
 !CLOSE ICCAP MAIN WINDOW
 !---------------------------
 ICCAP_FUNC("/", "Close")
 
 
