LINK XFORM "DELETE_ALL_INSTR" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 GLOBAL_VAR  ActString
 GLOBAL_VAR  Interface
 
 
 !DELETE GPIB INTERFACES & ALL INSTRUMENTS
 !---------------------------------------------------
 !==> Required ModelVariables: ActString, Interface
 
 !---first, the Instrument List needs to be empty
 ICCAP_FUNC("Hardware", "Clear Active List")
 
 !--define a temp. dirfilename
 dirfilename="tmp.hdw"
 
 !---write Hardware window to file
 ICCAP_FUNC("Hardware", "Save As", VAL$(dirfilename))
 
 !---open file for reading
 r_stream = USERC_open(VAL$(dirfilename),"r")
 
 !---find Keyword 'dev_file_list'
 Keyword="dev_file_list"
 ActString=""
 WHILE VAL$(ActString)<>VAL$(Keyword)
   dummy = USERC_readstr(r_stream,0,"%s",ActString)
 END WHILE
 
 !skip line with '{'
 dummy = USERC_readstr(r_stream,0,"%s",ActString)
 
 !read all until line with '}'
 DIMENSION_ARRAY("Interface", 0)
 i=0
 WHILE VAL$(ActString)<>"}"
   dummy = USERC_readstr(r_stream,0,"%s",ActString)
   IF VAL$(ActString) <>"}" THEN
     Interface="ICCAP_ARRAY["&VAL$(SIZE(Interface)+1)&"]"
     Interface[i]=ActString
     i = i + 1
   END IF
 END WHILE
 
 !---close file
 dummy = USERC_close(r_stream)
 
 !---delete Interfaces
 i=0
 WHILE i < SIZE(Interface)
   ICCAP_FUNC("Hardware", "Delete Interface File",VAL$(Interface[i]))
 i = i + 1
 END WHILE
 
 DIMENSION_ARRAY("Interface", 0)
 
 RETURN_VALUE 1
}
