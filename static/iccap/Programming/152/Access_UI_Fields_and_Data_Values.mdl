LINK XFORM "Create_GUIs_by_PEL" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 ! This is a best-practice example about how PEL can create GUIs.
 ! It is a general syntax, and therefore, it is recommended that
 ! you copy-paste the example below into your own Program,
 ! and adapt just the the GUI settings according to your requirements.
 ! The PEL flow can be kept as it is.
 
 
 
 !NOTE:	the GUI items for the ICCAP_FUNC command can be seen
 !	when manually activating button "Show Codes" in the GUI
 
 !NOTE: paths to the most upper GUI are per './',
 !	paths down the GUI hierarchy are per "."
 
 
 
 
 !HERE COMES A BEST_PRACTICE EXAMPLE FOR WRITING GUIs 
 ! USING PEL:
 !=================================================
 
 !First specify some useful local variables
 ParentName = "./demo_parent"
 ChildName = "demo_child"
 Path2Child = VAL$(ParentName)&"."&VAL$(ChildName)
 
 
 !=====Remove existing child GUI======================
 ICCAP_FUNC("./"&VAL$(Path2Child), "Delete")
 
 !=====Create a new child GUI (a table)===============
 ChildObject = "TL"	!###>Table
 ICCAP_FUNC(ParentName, "Add GUI", ChildName, ChildObject)
 
 !======Add Child1 to that table======================
 ParentName  =VAL$(ParentName)&"."&VAL$(ChildName)
 ChildName   ="CheckBox_1"
 ChildObject ="CB"	!###>Check Box
 ICCAP_FUNC(ParentName, "Add GUI", ChildName,ChildObject)
 
 Path2Child = VAL$(ParentName)&"."&VAL$(ChildName)
 !Specify Caption "this is a demo"
 ICCAP_FUNC(Path2Child, "Set GUI Options", "CA=this is a demo", "")
 
 
 !Specify Sensitivity 
 ! NOTE: a % before the Variable Name activates the 'Track Variable' tick box in the GUI
 sensitive = 1
 ICCAP_FUNC(Path2Child, "Set GUI Options", "SF=%sensitive", "")
 
 
 !Specify Toggle State Variable 'selected'
 ICCAP_FUNC(Path2Child,"Set GUI Options", "TS=%selected", "")
 
 
 !======Add Child2 to that table======================
 !Add another child to that table
 ChildName   ="Label_1"
 ChildObject ="LA"	!###>Label
 ICCAP_FUNC(ParentName, "Add GUI", ChildName, ChildObject)
 
 
 Path2Child=VAL$(ParentName)&"."&VAL$(ChildName)
 !Specify Caption "this is a demo"
 ICCAP_FUNC(Path2Child, "Set GUI Options", "CA=this is a label text", "")
 
 
 !Set GUI options
 fg_color="C1"    !text color		 C1=black, C2=red, C3=yellow, C4=green, C5=cyan, C6=blue, C7=magenta, C8=gray, C9=white, C10=medium blue
 bg_color="C10"   !field background color
 sizing_options="FW|FH"	!fixed hight and fixed width
 ICCAP_FUNC(Path2Child, "Set GUI Options", "FG="&VAL$(fg_color), "BG="&VAL$(bg_color), "TO="&VAL$(sizing_options), "")
 
 
 !======Finally, display the GUI
 ICCAP_FUNC("./demo_parent", "Display Modeless GUI", "DEMO")
 
