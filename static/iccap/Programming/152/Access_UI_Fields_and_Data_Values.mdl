LINK XFORM "Import" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 
 ! same handling like for 1-dimensional ICCAP_ARRAYs
 
 
 ! calling syntax
 
 	x = VAL$(array2[0][1])
 
 	GET_STRING "array2[0][1] = ", x, dummy
 
 
 !--------------------------------------------------------------------------------------
 ! EXAMPLE FOR IMPORTING A 2-DIMENSIONAL ARRAY from a remote Setup to the local Setup
 !--------------------------------------------------------------------------------------
 
 PathNameRemoteArray="specify-here"
 NameLocalArray="specify-here"
 
 !---first synchronize the MAIN size of the local ICCAP_ARRAY (1st array index) with the remote ICCAP_ARRAY size.
 MainArraySize=VAL(LOOKUP_VAR(VAL$(PathNameRemoteArray)))
 ICCAP_FUNC(".", "SetVariableTableValue",VAL$(NameLocalArray), "")
 ICCAP_FUNC(".", "SetVariableTableValue",VAL$(NameLocalArray), "ICCAP_ARRAY["&VAL$(MainArraySize)&"]")
 
 !---begin with importing the 1st array index
 imain=0
 WHILE imain < MainArraySize
   !---synchronize the SUB size of the local ICCAP_ARRAY (2nd array index) with the remote ICCAP_ARRAY size.
   SubArraySize=VAL(LOOKUP_VAR(VAL$(PathNameRemoteArray)&"["&VAL$(imain)&"]"))
   ICCAP_FUNC(".", "SetVariableTableValue",VAL$(NameLocalArray)&"["&VAL$(imain)&"]", "ICCAP_ARRAY["&VAL$(SubArraySize)&"]")
 
   !---do the import of the 2nd array index
   isub=0
   WHILE isub < SubArraySize
     ICCAP_FUNC(".", "SetVariableTableValue",VAL$(NameLocalArray)&"["&VAL$(imain)&"]["&VAL$(isub)&"]",LOOKUP_VAR(VAL$(PathNameRemoteArray)&"["&VAL$(imain)&"]["&VAL$(isub)&"]"))
     isub = isub + 1
   END WHILE
   imain = imain + 1
 END WHILE
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
LINK XFORM "Export" "711.501"
{
