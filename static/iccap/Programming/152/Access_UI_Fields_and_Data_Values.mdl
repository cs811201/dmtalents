{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 
 !----------------------------------------------------------------------
 ! EXAMPLE FOR EXPORTING A 2-DIMENSIONAL ARRAY from one Setup to another
 !----------------------------------------------------------------------
 
 PathNameLocalArray="specify-here"
 PathRemoteArray="specify-here"
 NameRemoteArray="specify-here"
 
 
 !---first synchronize the MAIN size of the remote ICCAP_ARRAY (1st array index) with your local ICCAP_ARRAY size.
 MainArraySize=VAL(LOOKUP_VAR(VAL$(PathNameLocalArray)))
 ICCAP_FUNC(VAL$(PathRemoteArray), "SetVariableTableValue", VAL$(NameRemoteArray), "")
 ICCAP_FUNC(VAL$(PathRemoteArray), "SetVariableTableValue", VAL$(NameRemoteArray), "ICCAP_ARRAY["&VAL$(MainArraySize)&"]")
 
 !---begin with exporting the 1st array index
 imain=0
 WHILE imain < MainArraySize
   !---synchronize the SUB size of the remote ICCAP_ARRAY (2nd array index) with your local ICCAP_ARRAY size.
   SubArraySize=VAL(LOOKUP_VAR(VAL$(PathNameLocalArray)&"["&VAL$(imain)&"]"))
   ICCAP_FUNC(VAL$(PathRemoteArray), "SetVariableTableValue", VAL$(NameRemoteArray)&"["&VAL$(imain)&"]", "ICCAP_ARRAY["&VAL$(SubArraySize)&"]")
 
   !---do the export of the 2nd array index
   isub=0
   WHILE isub < SubArraySize
     ICCAP_FUNC(VAL$(PathRemoteArray), "SetVariableTableValue", VAL$(NameRemoteArray)&"["&VAL$(imain)&"]["&VAL$(isub)&"]", LOOKUP_VAR(VAL$(PathNameLocalArray)&"["&VAL$(imain)&"]["&VAL$(isub)&"]"))
     isub = isub + 1
   END WHILE
   imain = imain + 1
 END WHILE
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
