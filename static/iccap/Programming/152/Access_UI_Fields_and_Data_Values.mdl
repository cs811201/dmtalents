LINK XFORM "README_FIRST" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 ! The examples in this Setup explain how to create GUIs by PEL programming.
 
 !NOTE:	the GUI items for the ICCAP_FUNC command can be seen
 !	when manually activating button "Show Codes" in the GUI
 
 !NOTE: paths to the most upper GUI are per '/',
 !	paths down the GUI hierarchy are per "."
 
 
 
 
 !Below are additional notes about writing GUIs by PEL 
 
 !Creating a GUI Item 
 !===================
 !You may add items to a GUI page via ICCAP_FUNC on either the GUI Items page, 
 !or another GUI item. 
 !Two additional parameters are required to name the item and to declare the item's type. 
 !The type specification must be a code which uniquely declares the Item type. 
 !Select the Show Codes button on the Properties dialog of a GUI Item 
 !to see the codes for all Item types. 
 
 ICCAP_FUNC("./GUIItems", "Add GUI", "NewTable", "TL")
 ICCAP_FUNC("./NewTable", "Add GUI", "Button", "PB")
 
 
 !Setting Options on a GUI Item 
 !=============================
 !You may change options on any Item on a GUI Page from PEL 
 !by using the Set GUI Options action on a GUI Item. 
 !You may enter as many additional arguments as desired. 
 !The last argument must be "" (a null string) to signify completion. 
 !Each argument is of the form <code>=<val>. 
 !Code is the associated code for the option as found by pressing 
 !the 'Show Codes' button on the properties dialog. 
 !The <val> can be one of four forms. 
 !
 ! A literal 
 !	<val> may be a string, or a number 
 !
 ! A Variable Changed 
 !	If the first character after the equal sign is a %, 
 !	the value is taken to track a variable by the given name. 
 !
 ! An Enumerated type code 
 !	Many options have enumerated types such as list selection mode. 
 !	Each of these possible values has an associated code which can be viewed 
 !	on the properties dialog. 
 !
 ! Multiple Enumerated types 
 !	Some options (sizing options, for example) allow multiple enumerated types 
 !	to be set (fixed width and fixed height). 
 !	These are indicated by specifying <code>|<code>. 
 !
 !Examples: 
 !title="test"                 ! set orientation to horizontal and caption to variable 'title'
 !ICCAP_FUNC("./someItem", "Set GUI Options", "OR=HO", "CA=%"&VAL$(title),"")
 !Set Sizing Options (many at once):
 !ICCAP_FUNC("./someItem", "Set GUI Options", "TO=FW|FH", "")
 
 
 !Setting Callbacks on a GUI Item 
 !===============================
 !You may set a callback on a GUI item by using the action 'Set GUI Callback'. 
 !In other words, specify the ICCAP_FUNC arguments inside a GUI item like a PushButton.
 !This action requires three other arguments. 
 !The first is the code for the callback. 
 !The second and third extra argument are the two parameters to the callbacks 
 !associated ICCAP_FUNC(). 
 
 !Example: Note: AC ['action'] is the PEL code for 'Button Pressed' in a Push Button GUI.
 !ICCAP_FUNC("./someItem", "Set GUI Callback", "AC", "path2transform", "Execute")
 
 !Anticipating GET_STRINGs and GET_xxxx commands in the called Macro/Transform:
 !ICCAP_FUNC("./someItem", "Set GUI Callback", "AC", "path2transform", "Execute,answer1,answer2")
 
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
LINK XFORM "add_delete_GUI_by_PEL" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program"
}
BLKEDIT "Program Body"
{
 !NOTE:	the keyword for the ICCAP_FUNC command can be seen
 !	when manually activating button "Show Codes" in the GUI
 
 !NOTE: paths to the most upper GUI are per './',
 !	paths down the GUI hierarchy are per "."
 
 
 
 
 
 !add a GUI child to an existing parent GUI
 !-----------------------------------------
 ParentObj="./myTable"       ! the Setup GUI myTable must exist !!
 TableName="myTableName"
 ICCAP_FUNC(ParentObj, "Add GUI", TableName, "TL")
 
 
 !delete the GUI 'toto', on top GUI level
 !----------------------------------------
 ICCAP_FUNC("./myTable", "Delete")
 
 
 
 
 
 ! Create a new GUI
 !--------------------------------
 modelPath="/my_ModelName"
 ParentGUIName   = "TopGUI"
 ParentGUIObject = "TL"
 ParentGUIPath   = VAL$(modelPath)&"/"&VAL$(ParentGUIName)
 ICCAP_FUNC(VAL$(modelPath)&"/GUIItems", "Add GUI", ParentGUIName, ParentGUIObject)
 ICCAP_FUNC(ParentGUIPath,"Set GUI Options", "CA=Parent Table", "OR=HO", "")
 
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
