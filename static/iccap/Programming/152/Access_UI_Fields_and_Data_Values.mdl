{
 
 
 !========
 ! EXPORT
 !========
   
 
 
 !syntax for setting a field in the Model Parameter table:
 
           !================================================================================
           !ICCAP_FUNC("/mdl/ParameterSet", "STFV", "ColumnName[ParamName]", "Value")
           !================================================================================
 
 !Note: "STFV" is an acronym for "SetTableFieldValue"
 
 
 
 ICCAP_FUNC("../../ParameterSet", "STFV", "Min[MAIN.RS]", "1m")
 ICCAP_FUNC("../../ParameterSet", "STFV", "Opt Min[MAIN.RS]", "10m")
 
 ! NOTE: the default parameter precision is 4 digits.
 !	If you want another precision, 
 !	apply the SystemVariable PARAMETER_PRECISION (best under IC-CAP/Main)
 !	and re-load the corresp. ModelFile
 ICCAP_FUNC("../../ParameterSet", "STFV", "Value[MAIN.RS]", "3.1415623")
 
 ICCAP_FUNC("../../ParameterSet", "STFV", "Opt Max[MAIN.RS]", "100")
 ICCAP_FUNC("../../ParameterSet", "STFV", "Max[MAIN.RS]", "1k")
 
 !Note: Go to the Model Parameters window to see the effects
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
