LINK XFORM "Create_a_Program" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 ! This is an example about how to create a new PEL Program by PEL
 
 !  Note:
 !  For Transforms, use 'Function' to enter to the 
 !  Transform's function field (enter 'Program' or 'Program2'),
 !  and use the access keyword 'Program' to fill out the PEL body.  
 
 
 !---create a new Transform by PEL
 ICCAP_FUNC(".", "Add Transform", "test")
 
 !---enter 'Program2' into Function field
 ICCAP_FUNC("test", "SetTableFieldValue", "Function", "Program2")
 
 
 
 
 
 !---now write your PEL (by PEL) into the Edit Field of 'Program' or 'Program2'.
 !   NOTE: the Edit Field is accessed in *both* cases by the keyword 'Program' !!!
 !   NOTE: your PEL lines are embedded within ' '  !!
 
 ICCAP_FUNC("test", "SetTableFieldValue", "Program", '! Here starts my auto-created PEL
 myText = "hello world"
 GET_STRING "Enter some text ",myText, myText
 
 RETURN_VALUE 1
 ')
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
