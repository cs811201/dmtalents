LINK XFORM "Import_ParamGroup" "711.501"
{
View block_edit 0 "0,0"
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 GLOBAL_VAR  ErrStr
 GLOBAL_VAR  ParamNames
 GLOBAL_VAR  avail_PARAMGROUP
 GLOBAL_VAR  avail_PARAMGROUP_sel
 GLOBAL_VAR  String2Array
 
 
 ! This Program identifies the Variables with the keyword PARAMGROUP_ in a
 ! user-selected ModelFile,
 ! lets the user select one or more parameter groups, 
 ! and finally provides an array with selected parameter names 'PARAM_NAMES'
 ! It also adds the selection 'Add All Param'.
 ! When the user selects this choice, Transform 'GET_NAMES_n_VALUES' is called by this Program
 ! -----------------------------------------------------------------------------------------------
  
 ! HINT:
 !========================================
 !   Grouping ModelParameters in a ModelFile
 !============###############==================
 !
 !- under ModelVariables, 
 !  - specify the   SYSTEM_VARIABLE   PARAMGROUP_xxxx
 !                                    ===============
 !       (xxxx = DC, CV, Spar etc., you can apply your own naming convention)
 !  - set its value to a space-delimited list of your selected parameter grouping  .
 !  - and the Model (and DUT) Parameters will be grouped.
 !
 !Example:
 !----------
 !PARAMGROUP_DC="RS  DMAIN.IS  DMAIN.N"
 !PARAMGROUP_CV="DMAIN.CJO  DMAIN.M  DMAIN.VJ  DMAIN.FC"
 !
 ! see also demo_features\5_PEL_PROGRAMMING\4_PARS_VARS_ARRAYS_STRINGS\grouping_Variables_n_Parameters.mdl
 
 
 
 
 !~~~~~~~~here it comes~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 
 
 GET_STRING "Enter ModelName", "Access_UI_Fields_and_Data_Values", ModelName
 
 
 !---clear arrays
 DIMENSION_ARRAY("avail_PARAMGROUP", 0)
 DIMENSION_ARRAY("avail_PARAMGROUP_sel", 0)
 DIMENSION_ARRAY("ParamNames", 0)
 
 
 !---find ModelVariables PARAMGROUP_xxxx  in specified Model
 N=0
 i=0
 ErrStr=""
 WHILE VAL$(ErrStr)==""
   ParamGroup=LOOKUP_TABLE_VAL("/"&VAL$(ModelName)&"/Variables", "Name["&VAL$(i)&"]", ErrStr)
   IF STRPOS(VAL$(ParamGroup), "PARAMGROUP_")>0 THEN
     DIMENSION_ARRAY("avail_PARAMGROUP", N+1)
     !---strip off prefix PARAMGOUP_
     ParamGroup = SUBSTR$(VAL$(ParamGroup), 12)
     !---add to array
     avail_PARAMGROUP[N]=VAL$(ParamGroup)
     N = N + 1
   END IF
   i = i + 1
 END WHILE
 ErrStr=""
 
 IF VAL(avail_PARAMGROUP)==0 THEN
   GET_STRING "No Variables with leading keyword 'PARAMGROUP_' found in loaded Model "&VAL$(ModelName)&"   [OK]", dummy
   RETURN_VALUE -1
 END IF
 
 
 
 
 !---open a GUI of found PARAMGROUPS and let user select
 ICCAP_FUNC("./TL_PARAMGROUP_Selection", "Display Modeless GUI", "Select Parameter Group")
 
 
 
 !---add parameters of selected PARAMGROUP to array 
 FOR iParamGroup=0 TO SIZEOF(avail_PARAMGROUP_sel)-1
   !---initialize
   String2Array=""
   !---calc. path to current selected PARAMGROUP-
   Path2Var="/"&VAL$(ModelName)&"/PARAMGROUP_"&VAL$(avail_PARAMGROUP[avail_PARAMGROUP_sel[iParamGroup]])
   !---get string of param names of currently selected PARAMGROUP_
   PARAMGROUP_Value=LOOKUP_VAR(VAL$(Path2Var))
   !---cut string chain into its components
   TmpVar = CUTSTR(VAL$(PARAMGROUP_Value), String2Array)
   !---get rid of ',' separators
   FOR i=0 TO SIZEOF(String2Array)-1
     String2Array[i]=TRIM$(VAL$(String2Array[i]), ",")
   NEXT i
 
   !---export selected parameters into ParamNames
   IndexOffset = SIZEOF(ParamNames)
   FOR iParam=0 TO SIZEOF(String2Array)-1
     DIMENSION_ARRAY("ParamNames", IndexOffset+(SIZEOF(String2Array)))
     ParamNames[IndexOffset+iParam]= VAL$(String2Array[iParam])
   NEXT iParam
 
 NEXT iParamGroup
 
 
 !---clear
 DIMENSION_ARRAY("String2Array", 0)
 
 
 RETURN_VALUE 1
 
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
data "711.501"
{
}
}
