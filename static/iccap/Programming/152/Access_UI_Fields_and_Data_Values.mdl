 IF VAL$(ErrMsg)<>"" THEN
   GET_STRING "ERROR: "&VAL$(ErrMsg)&"   [OK]", dummy
 END IF
 
 
 !---get x = 'Target Min' of 1st column in PlotOptimizer tab 'Inputs'
 x = LOOKUP_TABLE_VAL("/Access_UI_Fields_and_Data_Values/PlotOptimizer", "Target Min[0]", ErrMsg)
 IF VAL$(ErrMsg)<>"" THEN
   GET_STRING "ERROR: "&VAL$(ErrMsg)&"   [OK]", dummy
 END IF
 
 !---get x = 'Target Min' of 2nd column in PlotOptimizer tab 'Inputs'
 x = LOOKUP_TABLE_VAL("/Access_UI_Fields_and_Data_Values/PlotOptimizer", "Target Min[1]", ErrMsg)
 IF VAL$(ErrMsg)<>"" THEN
   GET_STRING "ERROR: "&VAL$(ErrMsg)&"   [OK]", dummy
 END IF
 
 
 RETURN_VALUE 1
}
