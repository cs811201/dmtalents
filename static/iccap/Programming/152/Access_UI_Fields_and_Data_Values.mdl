LINK XFORM "Overview" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 !PlotOptimizer
 !===================
 !To control the PlotOptimizer window,
 !use the following syntax (PlotOptimizer is one word): 
 !
 !To Open a PlotOptimizer:
 !    ICCAP_FUNC("/<model_name>", "Open PlotOptimizer")
 !or  ICCAP_FUNC("<plot_name>", "Open PlotOptimizer")
 !
 !To Close a PlotOptimizer:
 !    ICCAP_FUNC("/<model_name>/PlotOptimizer", "Close")
 !or  ICCAP_FUNC("<plot_name>/PlotOptimizer", "Close")
 !
 !------
 !To add a Plot to the PlotOptimizer:
 !    ICCAP_FUNC(plotname, "AutoconfigureAndEnable")
 !
 !
 !Once the PlotOptimizer window is open, use
 !
 !    ICCAP_FUNC("/<model_name>/PlotOptimizer", "<action_name>", ["<value>"]) 
 !or  ICCAP_FUNC("<plot_name>/PlotOptimizer",   "<action_name>", ["<value>"]) 
 !
 !Examples:
 !ICCAP_FUNC("/npn/PlotOptimizer", "Simulate All")  
 !
 !Further actions: 
 !===================
 !- "Auto Set Min Max"	!applying SystemVariable AUTOSET_COEFF
 !- "Autoset Min" 
 !- "Autoset Max" 
 !- "Reset Min Max"		!setting to default param. limits or to 'Opt Min' and 'Opt Max' in tab 'Model Parameters'
 !- "Reset Min" 
 !- "Reset Max" 
 
 !- "Read from file"
 !- "Write to file"
 !- "Clear Plot Optimizer"
 !- "Clear Table" or "Clear Parameter Table"
 !- "Disable All"
 !- "Enable All"
 !- "Optimize"
 !- "Recall Parameters"
 !- "Reset Option Table"
 !- "Simulate All"
 !- "Simulate Plot Inputs"
 !- "Store Parameters"
 !- "Tune Fast"
 !- "Tune Slow"
 !- "Undo Optim"
 ! 
 !- "Set Algorithm"
 !Sets the optimization algorithm to one of the following choices: 
 !Gradient			B
 !Minimax			N
 !Levenberg-Marquardt	L
 !Sensitivity Analysis	S
 !Hybrid (Random/LM)		H
 !Random			R
 !Random (Gucker)		A
 !Random Minimax		U
 !Gradient Minimax		E
 !Quasi-Newton		G
 !Least Pth		I
 !Hybrid (Random/Quasi-Newton)	P
 !Genetic			Q
 
 !Note: recommended algorithm sequence:  Gradient (abs -> rel)  -> Levenberg-Marquardt (abs -> rel)
 
 
 
 !- "Set Error"  "Absolute" or "Relative"
 
 
 
 !PlotOptimizer actions are also available directly in the Plot object itself: 
 !============================================================================
 !Example:
 !ICCAP_FUNC("<path2plot>", "Open PlotOptimizer")
 !available actions:
 !- "Open PlotOptimizer"
 !- "Enable Plot" 
 !- "Disable Plot" 
 !- "AutoconfigureAndEnable" 
 
 
 !Examples:
 !==========
 !ICCAP_FUNC("/modelname/PlotOptimizer", "SetAlgorithm", "Gradient")
 !ICCAP_FUNC("/modelname/DUTname/Setupname/Plotname/PlotOptimizer", "SetAlgorithm", "Gradient")
 
 !ICCAP_FUNC("modelname/PlotOptimizer", "SetError", "Absolute")
 
 
 
 ! To add global regions, independent of the axis settings (!):
 !=======================================================================
 !                                              low x, high x, low y, high y
 !ICCAP_FUNC("<plot_name>", "AddGlobalNativeRegion", "0.6", "0.8", "-8", "-3")
 
 !to delete global regions, apply
 !ICCAP_FUNC("<plot_name>", "DeleteGlobalRegions")
 
 ! and if your Plot is within a MultiPlot, specify additionally the Plot number,
 ! e.g.: Specify plot number for Multiplot 
 !                                           low x, high x, low y, high y  PlotID
 ! ICCAP_FUNC("<multi_plot_name>", "AddGlobalRegion", "0.6", "0.8",  "-8",  "-3",   "1")
 ! and correspondingly
 ! ICCAP_FUNC("<multi_plot_name>", "DeleteGlobalRegions", "1")
 
 
 
 ! To clear the PlotOptimizer:
 !====================================
 !ICCAP_FUNC("<plot_name>/PlotOptimizer", "Clear Plot Optimizer")
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
