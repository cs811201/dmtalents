HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 !This is a demo about how to change a plot header, 
 !or how to change entry fields in Inputs or Outputs.
 
 
 !Accessing table entries for Inputs, Outputs and Plots:
 ! SYNTAX:
 ! ICCAP_FUNC("path2plot", "SetTableFieldValue", "Field2change", "a_string")
 ! or
 ! ICCAP_FUNC("path2plot", "STFV", "Field2change", "a_string")
 
 
 !---EXAMPLE
 !---------------------------
 ICCAP_FUNC("ia_va", "Display Plot")
 GET_STRING "enter header text", "test", header_text
 
 !-----------------------------------------------------------------------
 ICCAP_FUNC("ia_va", "STFV", "Header", VAL$(header_text))
 !-----------------------------------------------------------------------
 
 ICCAP_FUNC("ia_va", "Display Plot")
 
 
 ! more examples for plot manipulations:
 ! ======================================
  ICCAP_FUNC("ia_va", "SetTableFieldValue", "Y Axis Type", "LIN")
 ! NOTE: for the logarithmic scale apply "LOG", for dB apply "DB"
 ! Additionally, to make sure that the Plot looses his history settings, and respects the new 'parent' settings !!!
 ! ICCAP_FUNC("ia_va", "Reset to Saved Options") 
 
 ! ICCAP_FUNC("ia_va", "SetTableFieldValue", "Y Data 1", "ia.s")
 
 
 
 !Note:
 !===============================================
 ! the 'Header' and 'Footer' entries in a plot 
 ! are assuming a string entry. 
 ! Therefore, they cannot be linked directly 
 ! to an IC-CAP variable.
 ! If you entered a variable, then, instead of printing 
 ! the value/string of the entered IC-CAP variable in
 ! the opened Plot, the variable name will be printed 
 ! in the plot header instead!
 
 
 
 
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
