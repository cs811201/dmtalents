LINK XFORM "Import" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 !========
 !IMPORT: 
 !========
 
 !FEATURE: LOOKUP_VAR() allows to access the value of a variable 
 !	referenced by a string.  A second arg may be
 !	specified which is a variable to receive any error string normally
 !	going to a red error box.
 
 
 !Importing from an individual member of an ICCAP_ARRAY
 !=====================================================
 
 !EXAMPLE:
 	  
           path2variable="./test[1]"
 
           !========================================!
            x=LOOKUP_VAR(path2variable)
           !========================================!
 
           GET_STRING "the variable array1[1] is currently set to: "&VAL$(x),"ok",dummy
 
 
 !ERROR HANDLING:
 !          x=LOOKUP_VAR("nonexistantvar", errstr)
 !          if errstr<>"" then print errstr
 
 
 
 
 !Importing a complete ICCAP_ARRAY(containing numbers)
 ! i.e. importing into an array specified by a COMPLEX statement
 !===============================================================
 ! This is a bit more tricky:
 !The following PEL Transform example is about finding out 
 !the SIZE of the remote ICCAP_ARRAY, and then to link 
 !that remote ICCAP_ARRAY into the PEL program
 
 remote_iccap_array="./test"
 
 !read the ICCAP_ARRAY size
 remote_iccap_array_size=VAL(LOOKUP_VAR(VAL$(remote_iccap_array)))
 
 !now link the array
 COMPLEX tmp[remote_iccap_array_size]
 i=0
 WHILE i < SIZE(tmp)
   tmp[i]=VAL(LOOKUP_VAR(VAL$(remote_iccap_array)&"["&VAL$(i)&"]"))
   i = i + 1
 END WHILE
 
 PRINT tmp
 
 
 
 
 !Importing a complete ICCAP_ARRAY (containing strings)
 ! i.e. importing into a local ICCAP_ARRAY 'local_iccap_array'
 !===============================================================
 
 myArray=""
 !                    from         to
 RetVal=ARRAY_COPY(./test1, ./myArray)
 
 
 
 
 
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
