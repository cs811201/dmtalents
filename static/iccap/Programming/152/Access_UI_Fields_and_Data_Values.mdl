LINK XFORM "Import_Netlist" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 
 !---import the netlist from Tab 'Circuit'
 myCircuitText = LOOKUP_TABLE_VAL("../../Circuit", "Text")	!"Text" is the keyword to get all the Macro PEL code into the Variable 'x'
 
 
 !---import the netlist from Tab 'Test Circuit' of current DUT
 myCircuitText = LOOKUP_TABLE_VAL("../TestCircuit", "Text")	!"Text" is the keyword to get all the Macro PEL code into the Variable 'x'
 
 
 
 !---dump to Status Window
 PRINT myCircuitText
 
 
 
 
 
 RETURN_VALUE 1
 
 
 
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
LINK XFORM "Export_Netlist" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 
 
 !---create your netlist
 myCircuitText = ".SUBCKT test 1 2
 Rtest 1 2 1k
 .ENDS"
 
 
 
 !---export it to tab 'Circuit'       "Text" is the keyword to get all the Macro PEL code into the Variable 'x'
 ICCAP_FUNC("../../Circuit", "STFV", "Text", VAL$(myCircuitText))	
 
 
 !---export it to tab 'Test Circuit' of current DUT      "Text" is the keyword to get all the Macro PEL code into the Variable 'x'
 ICCAP_FUNC("../TestCircuit", "STFV", "Text", VAL$(myCircuitText))	
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
