LINK XFORM "Delete" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 
 !=========
 ! DELETE:
 !=========
 ! Apply "SetTableFieldValue" or "STFV" to set *Name* and *Field* of a variable.
 ! This means also, the by setting a name to an empty string, the variable can be deleted.
 
 ! NOTE:  Path to the VariableTable is / for ICCAP/Main
 !               or  /model[/DUT/[Setup]]
 
 !EXAMPLE:
 
 GET_STRING "enter the name of a Variable to be deleted", "test", VarName
 GET_STRING "enter path to the Variable ('.'  or  '/model[/DUT/[Setup]]')", ".", Path2Setup
 
 
 !===================================================================================================
 !Very important, first check, if this Variable really exists !!!
 ErrStr=""
 dummy=LOOKUP_VAR(VAL$(Path2Setup)&"/"&VAL$(VarName), ErrStr)
 
 IF VAL$(ErrStr)=="" THEN
   ICCAP_FUNC(VAL$(Path2Setup), "SetTableFieldValue", "Name["&VAL$(VarName)&"]", "") 
 ELSE
   GET_STRING "ERROR: the requested Variable '"&VAL$(VarName)&"' does not exist and thus, cannot be deleted",dummy
 END IF
 !===================================================================================================
 
 ! This STFV-function will search for the Variable with the Name-Field 
 ! equal to a string (or in the example above, equal to VAL$(mdlvarname)
 ! and will then set it to an empty space. 
 ! This will cause the variable to be deleted from the table.
 
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 0 0
}
}
}
LINK XFORM "Get_names_and_Values" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 GLOBAL_VAR  VarNames
 GLOBAL_VAR  VarValues
 
 
 
 !=======================================
 ! GET NAME AND VALUE OF A VARIABLE TABLE
 !=======================================
 
 ! LOOKUP_TABLE_VAL()  or  LOOKUP_TABLE_VAL_EVAL()
 !
 ! Enables you to access the value of a field in an input, output, plot, 
 ! parameter, or optimizer table. 
 ! The first argument is the path to the table. 
 ! The second argument is the field in the table . 
 ! An optional third argument may be specified which is a variable 
 !   to receive any error string normally going to a red error box. 
 !->  LOOKUP_TABLE_VAL returns NUMPTS if NUMPTS is a variable in # of Points. 
 !->  LOOKUP_TABLE_VAL_EVAL returns 100 (presuming NUMPTS evaluates to 100).
 
 
 !~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 !---Example
 
 GET_STRING "Path to Variables, e.g. ../.. (to refer to ModelVariables)", "../..", Path2VarTable
 
 ! NOTE:  Path to the VariableTable is / for ICCAP/Main
 !               or  /model[/DUT/[Setup]]
 
 DIMENSION_ARRAY("VarNames", 0)
 DIMENSION_ARRAY("VarValues", 0)
 N=0
 i=0
 ErrStr=""
 WHILE VAL$(ErrStr)==""
   VarName =LOOKUP_TABLE_VAL(VAL$(Path2VarTable), "Name["&VAL$(i)&"]", ErrStr)
   VarValue=LOOKUP_TABLE_VAL(VAL$(Path2VarTable), "Value["&VAL$(i)&"]", ErrStr)
   IF (VAL$(ErrStr)=="") ANDALSO (VAL$(VarName)<>"") ANDALSO (STRPOS(VAL$(VarName), "[")==0) THEN
     DIMENSION_ARRAY("VarNames", N+1)
     DIMENSION_ARRAY("VarValues", N+1)
     VarNames[N] =VarName
     IF STRPOS(VAL$(VarValue), "[")>0 THEN
       VarValues[N] = "ICCAP_ARRAY"
     ELSE
       VarValues[N]=VarValue
     END IF
     N = N + 1
   END IF
   i = i + 1
 END WHILE
 
 ErrStr=""
 
 GET_STRING "The identified Variables and their values are in SetupVariables, 
 array 'VarNames' and 'VarValues'  [OK]", dummy
 
 
 RETURN_VALUE 1
}
dataset
{
datasize COMMON 1 1 1
type COMMON
point 0 1 1 1 0
}
}
}
