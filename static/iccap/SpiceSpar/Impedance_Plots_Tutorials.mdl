LINK XFORM "tuning" "711.501"
{
data
{
HYPTABLE "Link Transform"
{
element "Function" "Program2"
}
BLKEDIT "Program Body"
{
 UPDATE_EXPLICIT
 
 
 GLOBAL_VAR  Params
 GLOBAL_VAR  Mins
 GLOBAL_VAR  Inits
 GLOBAL_VAR  Maxs
 
 
 !---this Program is called by the different Spice Schematics Transforms
 
 
 !---initialize Tuner Variables
 Params=""
 Mins=""
 Inits=""
 Maxs=""
 
 
 !---reset ModelParameters, simulate and show Plot
 ICCAP_FUNC("../../ParameterSet", "Reset")
 ICCAP_FUNC(".", "Simulate")
 ICCAP_FUNC("_MP_", "Display Plot")
 
 
 !---set TUNER-----------------------------------------------------
 !---get ModelParameter Names
 N=0
 ErrMsg=""
 WHILE VAL$(ErrMsg)==""
   ParamName=LOOKUP_TABLE_VAL("../../ParameterSet", "Param Name["&VAL$(N)&"]", ErrMsg)
 !  PRINT ParamName
   IF VAL$(ErrMsg)=="" THEN
     DIMENSION_ARRAY("Params", N+1)
     Params[N]=VAL$(ParamName)
   END IF
   N = N + 1
 END WHILE
 NrParams = N - 1
 ErrMsg=""
 
 
 !---set limits for Tuner
 DIMENSION_ARRAY("Mins" , NrParams)
 DIMENSION_ARRAY("Inits", NrParams)
 DIMENSION_ARRAY("Maxs" , NrParams)
 FOR i=0 TO NrParams-1
   !---get ModelParameter Value and set Tuner arrays
   ParamValue = LOOKUP_TABLE_VAL("../../ParameterSet", "Value["&VAL$(Params[i])&"]")
   Inits[i]  = VAL(ParamValue)
   Mins[i]   = Inits[i]//100
   Maxs[i]   = Inits[i]*100
 NEXT i
 
 
 !---call TUNER
 !                                1=log scale, 0=lin scale
 !                                   1=immediate execution, 0=execute after slider release
 !                                      program name to be executed
 TUNER Params, Mins, Inits, Maxs, 1, 1, "aux"
}
