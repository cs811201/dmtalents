          ICCAP_FUNC("/UTILITIES/PEL_UTILITIES/STOP_PEL_EXECUTION/StopButton_Check", "Execute")
         END IF
         IF VAL$(/UTILITIES/PEL_UTILITIES/STOP_PEL_EXECUTION/RequestedStopAction) == "STOP" THEN BREAK
         !##########################################################################################################
 
 
       NEXT iMeasure
 
 
       !##########################################################################################################
       IF VAL$(/UTILITIES/PEL_UTILITIES/STOP_PEL_EXECUTION/RequestedStopAction) == "STOP" THEN BREAK
