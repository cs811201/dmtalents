   GET_STRING "Change .mdm File Name     [OK], [Cancel]", FileName, FileNameNew
 
   dummy=SYSTEM("cd "&VAL$(ActDirPath) &"
 mv "&VAL$(FileName)&" "&VAL$(FileNameNew)&" 2> /dev/null")
 
   ICCAP_FUNC("Show_Dirs_Files", "Execute")
   RETURN_VALUE 6
 END IF
 
 
 RETURN_VALUE 1
 
}
