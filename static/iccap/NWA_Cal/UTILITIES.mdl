 GLOBAL_VAR  SimulFlag
 GLOBAL_VAR  xAxisChoices
 GLOBAL_VAR  xAxisChoicesSel
 !~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 IF DomainSel < 0 THEN RETURN_VALUE 2
 
 
 
 !---close MultiPlot
 ICCAP_FUNC("..", "Close All")
 
 
