   IF MEAN(S_deemb.M.11)<>0 THEN tmp.M[i] = 10^(PowerGain.M[i]//10)
   IF MEAN(S_deemb.S.11)<>0 THEN tmp.S[i] = 10^(PowerGain.S[i]//10)
   i = i + 1
 END WHILE
 
 
 RETURN SQRT(tmp)*freq
}
