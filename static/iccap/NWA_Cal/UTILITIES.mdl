 IF StopExec==1 THEN RETURN_VALUE 0
 !----------------------------------------
 
 GLOBAL_VAR  Debug		!a DUT Variable
 IF VAL(Debug)==1 THEN PRINT "exec.Xfm "&WHOAMI()&"..."
 
 
 !------------------------------------------------------------------------------
 !---calculating Gain = Pout(fundamental)[dBm] - pin[dBm] 
 !------------------------------------------------------------------------------
 
 
 !---ask selected x-Axis
 xAxis = VAL$(xAxisChoices[xAxisChoicesSel])
